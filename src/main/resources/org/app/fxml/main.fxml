<?xml version="1.0" encoding="UTF-8"?>

<?import atlantafx.base.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="1080.0" prefWidth="1220.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.controller.MainController">
    <ModalPane fx:id="modal" />
    <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="1080.0" prefWidth="1220.0">
        <top>
            <fx:include source="menubar.fxml" />
        </top>
        <right>
            <fx:include source="stats.fxml" />
        </right>
        <center>
            <TabPane fx:id="tabPane" side="BOTTOM" tabClosingPolicy="UNAVAILABLE" tabMinWidth="70.0">
                <Tab closable="false" style="-fx-padding: 8px;" text="Trades">
                    <fx:include source="trades.fxml" />
                </Tab>
                <Tab closable="false" style="-fx-padding: 8px;" text="Daily Prep">
                    <fx:include source="dailyPrep.fxml" />
                </Tab>
                <Tab closable="false" style="-fx-padding: 8px;" text="Journal">
                    <fx:include source="Journal.fxml" />
                </Tab>
                <Tab closable="false" style="-fx-padding: 8px;" text="Goals">
                    <fx:include source="Goals.fxml" />
                </Tab>
                <Tab closable="false" style="-fx-padding: 8px;" text="Equity Chart">
                    <fx:include source="chart.fxml"/>
                </Tab>
            </TabPane>
        </center>
    </BorderPane>
    <BorderPane.margin>
        <Insets />
    </BorderPane.margin>
</StackPane>
